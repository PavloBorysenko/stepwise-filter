{
    "name": "na-gora/stepwise-filter",
    "description": "filter for woocommerce",
    "type": "project",
    "autoload": {
        "psr-4": {
            "NaGora\\StepwiseFilter\\": "include/"
        }
    },
    "authors": [
        {
            "name": "PavloBorysenko",
            "email": "pablodevelophelp@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "10",
        "yoast/phpunit-polyfills": "^2.0",
        "woocommerce/woocommerce-sniffs": "^1.0",
        "phpstan/phpstan": "^2.1",
        "szepeviktor/phpstan-wordpress": "^2.0",
        "phpstan/extension-installer": "^1.4"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true
        }
    }
}
